{
  "name": "suryadarmaputra",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "build:netlify": "NETLIFY_DEPLOYMENT=true react-router build",
    "commit": "cz",
    "dev": "react-router dev",
    "format:fix": "prettier --write",
    "generate": "bun run generate:posts && bun run generate:projects",
    "generate:posts": "bun scripts/fetch-posts.ts",
    "generate:posts:unpublished": "SHOW_UNPUBLISHED=true bun scripts/fetch-posts.ts",
    "generate:projects": "bun scripts/fetch-projects.ts",
    "generate:campaigns": "bun scripts/fetch-campaigns.ts",
    "lint": "eslint --max-warnings=0 --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint",
    "lint:fix": "bun run lint --fix",
    "start": "react-router-serve ./build/server/index.js",
    "sync": "bun scripts/sync-with-notion.ts",
    "typecheck": "react-router typegen && tsc",
    "versioning": "standard-version"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "@react-router/node": "^7.0.0",
    "@react-router/serve": "^7.0.0",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-lazyload": "^3.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "canvas": "^3.0.0-rc2",
    "cssnano": "^7.1.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "embla-carousel": "^8.6.0",
    "embla-carousel-auto-scroll": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "image-js": "^0.35.6",
    "isbot": "^4.1.0",
    "lodash": "^4.17.21",
    "react": "^19.1.1",
    "react-codepen-embed": "^1.1.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^19.1.1",
    "react-gistlab": "^1.0.8",
    "react-icons": "^5.4.0",
    "react-lazyload": "3.2.0",
    "react-router": "^7.9.1",
    "react-syntax-highlighter": "^15.6.1",
    "react-twitter-widgets": "^1.11.0",
    "react-zoom-pan-pinch": "^3.6.1",
    "reading-time": "^1.5.0",
    "sharp": "^0.33.5",
    "superagent": "^10.1.1",
    "tailwind-merge": "^2.6.0",
    "undici": "^7.1.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@netlify/vite-plugin-react-router": "^1.0.1",
    "@react-router/dev": "^7.9.1",
    "@react-router/fs-routes": "^7.9.1",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/lodash": "^4.17.13",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/superagent": "^8.1.9",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "commitizen": "^4.3.1",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "lefthook": "^1.8.4",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "standard-version": "^9.5.0",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}