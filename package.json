{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "yarn run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css --postcss",
    "commit": "cz",
    "dev": "concurrently \"yarn run dev:css\" \"netlify dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css --postcss",
    "format:fix": "prettier --write",
    "generate": "ts-node scripts/fetchNotionData.ts",
    "lint": "eslint --ext .ts,.tsx --max-warnings=0",
    "lint:fix": "eslint --ext .ts,.tsx --max-warnings=0 --fix",
    "postinstall": "remix setup node",
    "sync": "ts-node scripts/syncWithNotion.ts",
    "versioning": "standard-version"
  },
  "dependencies": {
    "@netlify/functions": "^1.0.0",
    "@notionhq/client": "^1.0.4",
    "@remix-run/netlify": "1.2.3",
    "@remix-run/react": "^1.2.3",
    "@types/lodash": "^4.14.181",
    "@types/xml2js": "^0.4.9",
    "dayjs": "^1.11.0",
    "image-js": "^0.33.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-codepen-embed": "^1.1.0",
    "react-dom": "^17.0.2",
    "react-gistlab": "^1.0.8",
    "react-lazyload": "^3.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-twitter-widgets": "^1.10.0",
    "react-zoom-pan-pinch": "^2.1.3",
    "reading-time": "^1.5.0",
    "remix": "^1.2.3",
    "superagent": "^7.1.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/cz-commitlint": "^16.2.3",
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.2.3",
    "@remix-run/serve": "^1.2.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-lazyload": "^3.2.0",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/superagent": "^4.1.15",
    "autoprefixer": "^10.4.4",
    "commitizen": "^4.2.4",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "standard-version": "^9.3.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}